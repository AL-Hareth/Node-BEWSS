{
  "name": "bewss",
  "version": "1.0.0",
  "description": "Tool to make interfacing with websockets on MCBE easy!",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "engines": {
    "node": "<=14.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "build": "tsc",
    "buildapp": "pkg .",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**"
  },
  "pkg": {
    "assets": "src/bewss/@interface/bewss.i.ts",
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-windows-x64"
    ],
    "outputPath": "build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "chalk": "^4.1.0",
    "child_process": "^1.0.2",
    "events": "^3.2.0",
    "fs-extra": "^10.0.0",
    "moment": "^2.29.1",
    "readline": "^1.3.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^14.14.21",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "nodemon": "^2.0.7",
    "pkg": "^5.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.2"
  }
}
